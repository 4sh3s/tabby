<config xmlns="http://www.ehcache.org/v3">

    <cache alias="methods">
        <key-type>java.lang.String</key-type>
        <value-type>tabby.db.bean.ref.MethodReference</value-type>
        <expiry>
            <ttl unit="minutes">2</ttl>
        </expiry>

        <resources>
            <heap unit="GB">3</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache>

    <cache alias="classes">
        <key-type>java.lang.String</key-type>
        <value-type>tabby.db.bean.ref.ClassReference</value-type>
        <expiry>
            <ttl unit="minutes">2</ttl>
        </expiry>

        <resources>
            <heap unit="GB">1</heap>
            <offheap unit="MB">10</offheap>
        </resources>
    </cache>

</config>